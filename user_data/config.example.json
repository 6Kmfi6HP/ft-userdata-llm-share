{
    "$schema": "https://schema.freqtrade.io/schema.json",
    "max_open_trades": 5,
    "stake_currency": "USDT",
    "stake_amount": "unlimited",
    "tradable_balance_ratio": 0.99,
    "fiat_display_currency": "USD",
    "dry_run": true,
    "dry_run_wallet": 1340,
    "cancel_open_orders_on_exit": false,
    "trading_mode": "futures",
    "margin_mode": "isolated",
    "unfilledtimeout": {
        "entry": 5,
        "exit": 5,
        "exit_timeout_count": 0,
        "unit": "minutes"
    },
    "entry_pricing": {
        "price_side": "other",
        "use_order_book": true,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },
    "exit_pricing": {
        "price_side": "other",
        "use_order_book": true,
        "order_book_top": 1
    },
    "order_types": {
        "entry": "limit",
        "exit": "market",
        "emergency_exit": "market",
        "force_exit": "market",
        "force_entry": "market",
        "stoploss": "market",
        "stoploss_on_exchange": false
    },
    "trailing_stop": false,
    "exchange": {
        "name": "binance",
        "key": "your-binance-api-key",
        "secret": "your-binance-api-secret",
        "ccxt_config": {
            "enableRateLimit": true,
            "options": {
                "defaultType": "future"
            }
        },
        "ccxt_async_config": {
            "enableRateLimit": true,
            "rateLimit": 200,
            "timeout": 30000
        },
        "pair_whitelist": [],
        "pair_blacklist": []
    },
    "pairlists": [
        {
            "method": "VolumePairList",
            "number_assets": 5,
            "sort_key": "quoteVolume",
            "min_value": 0,
            "refresh_period": 900
        },
        {
            "method": "AgeFilter",
            "min_days_listed": 3
        }
    ],
    "telegram": {
        "enabled": false,
        "token": "your-telegram-bot-token",
        "chat_id": "your-telegram-chat-id"
    },
    "discord": {
        "enabled": false,
        "webhook_url": "your-discord-webhook-url",
        "allow_custom_messages": true,
        "exit_fill": [
            {
                "Trade ID": "{trade_id}"
            },
            {
                "Exchange": "{exchange}"
            },
            {
                "Pair": "{pair}"
            },
            {
                "Direction": "{direction}"
            },
            {
                "Open rate": "{open_rate}"
            },
            {
                "Close rate": "{close_rate}"
            },
            {
                "Amount": "{amount}"
            },
            {
                "Open date": "{open_date:%Y-%m-%d %H:%M:%S}"
            },
            {
                "Close date": "{close_date:%Y-%m-%d %H:%M:%S}"
            },
            {
                "Profit": "{profit_amount} {stake_currency}"
            },
            {
                "Profitability": "{profit_ratio:.2%}"
            },
            {
                "Enter tag": "{enter_tag}"
            },
            {
                "Exit Reason": "{exit_reason}"
            },
            {
                "Strategy": "{strategy}"
            },
            {
                "Timeframe": "{timeframe}"
            }
        ],
        "entry_fill": [
            {
                "Trade ID": "{trade_id}"
            },
            {
                "Exchange": "{exchange}"
            },
            {
                "Pair": "{pair}"
            },
            {
                "Direction": "{direction}"
            },
            {
                "Open rate": "{open_rate}"
            },
            {
                "Amount": "{amount}"
            },
            {
                "Open date": "{open_date:%Y-%m-%d %H:%M:%S}"
            },
            {
                "Enter tag": "{enter_tag}"
            },
            {
                "Strategy": "{strategy} {timeframe}"
            }
        ]
    },
    "api_server": {
        "enabled": true,
        "listen_ip_address": "0.0.0.0",
        "listen_port": 8080,
        "verbosity": "error",
        "enable_openapi": true,
        "jwt_secret_key": "your-jwt-secret-key-generate-a-random-string",
        "ws_token": "your-websocket-token-generate-a-random-string",
        "CORS_origins": [],
        "username": "freqtrader",
        "password": "your-secure-password"
    },
    "strategy": "LLMFunctionStrategy",
    "bot_name": "LLM-Function-Strategy-v1",
    "initial_state": "running",
    "force_entry_enable": true,
    "position_adjustment_enable": true,
    "max_entry_position_adjustment": 10,
    "process_only_new_candles": true,
    "internals": {
        "process_throttle_secs": 3
    },
    "llm_config": {
        "api_base": "http://host.docker.internal:3120",
        "api_key": "sk-your-llm-api-key",
        "model": "qwen/qwen3-30b-a3b-thinking-2507"
    },
    "risk_management": {
        "max_leverage": 100,
        "default_leverage": 10,
        "max_position_pct": 50,
        "max_open_trades": 4,
        "allow_model_freedom": true,
        "emergency_stop_loss": -0.15
    },
    "experience_config": {
        "log_decisions": true,
        "log_trades": true,
        "decision_log_path": "./user_data/logs/llm_decisions.jsonl",
        "trade_log_path": "./user_data/logs/trade_experience.jsonl",
        "max_recent_trades_context": 5,
        "max_recent_decisions_context": 10,
        "include_pair_specific_trades": true,

        "enable_rag": true,
        "rag_config": {
            "embedding": {
                "model_name": "text-embedding-bge-m3",
                "api_url": "http://host.docker.internal:3120",
                "api_key": "sk-your-embedding-api-key",
                "api_type": "openai",
                "dimension": 1024,
                "batch_size": 8
            },
            "vector_store": {
                "index_type": "flat",
                "storage_path": "./user_data/rag/vector_store"
            },
            "reward_learning": {
                "learning_rate": 0.1,
                "discount_factor": 0.95,
                "storage_path": "./user_data/logs/reward_learning.json"
            }
        }
    },
    "context_config": {
        "max_context_tokens": 6000,
        "system_prompt_tokens": 500,
        "market_data_tokens": 800,
        "rag_history_tokens": 1500,
        "enable_context_compression": true,
        "include_timeframe_alignment": false,
        "include_timeframe_guidance": false,
        "indicator_history_points": 80,
        "indicator_history_lookback": 160,
        "raw_kline_history_points": 80,
        "raw_kline_max_rows": 80,
        "raw_kline_stride": 1,
        "raw_kline_compact_format": true,
        "raw_kline_extra_fields": [
            "rsi",
            "macd",
            "atr"
        ],
        "multi_timeframe_compact_format": true,
        "multi_timeframe_max_rows": 120,
        "default_multi_timeframe_fields": [
            "ema_20",
            "ema_50",
            "rsi",
            "macd",
            "macd_signal",
            "macd_hist",
            "atr",
            "adx"
        ],
        "multi_timeframe_history": {},
        "include_multi_timeframe_data": false
    },
    "custom_stoploss_config": {
        "profit_thresholds": [0.02, 0.06, 0.15],
        "atr_multipliers": [1.5, 1.0, 0.8],
        "min_distances": [0.004, 0.0025, 0.002],
        "time_decay_hours": 2,
        "time_decay_factor": 0.8,
        "trend_strength_threshold": 25,
        "trend_strength_factor": 1.2,
        "use_smooth_transition": true,
        "max_atr_pct": 0.10,
        "trend_priority": true,
        "direction_tolerance": 0.0001,
        "min_stop_distance": 0.0001,
        "max_stop_distance": 0.50
    },
    "custom_exit_config": {
        "extreme_profit_threshold": 0.50,
        "extreme_rsi_threshold": 90,
        "exceptional_profit_threshold": 0.70,
        "use_leverage_adjusted_thresholds": false,
        "profit_epsilon": 0.001
    }
}
